<Window x:Class="WPF_AG_CSG.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:local="clr-namespace:WPF_AG_CSG"
        mc:Ignorable="d"
        Title="MainWindow" Height="720" Width="1280" ResizeMode="NoResize" Background="Transparent" WindowStyle="None">
    <materialDesign:Card UniformCornerRadius="20" materialDesign:ElevationAssist.Elevation="Dp12">
        <Grid>
            <Grid.Background>
                <ImageBrush ImageSource="../../Resources/Assets/DU_14_BUS.png"/>
            </Grid.Background>
            <Button Command="{Binding GetGenParametersCommand}" Background="Transparent" Cursor="Hand" CommandParameter="Gen_0001" HorizontalAlignment="Left" Height="45" Margin="90,210,0,0" VerticalAlignment="Top" Width="58"/>
            <Button Command="{Binding GetGenParametersCommand}" Background="Transparent" Cursor="Hand" CommandParameter="Gen_0002" HorizontalAlignment="Left" VerticalAlignment="Top" Width="58" Margin="290,544,0,0" Height="40"/>
            <Button Command="{Binding GetGenParametersCommand}" Background="Transparent" Cursor="Hand" CommandParameter="Gen_0003" HorizontalAlignment="Left" Height="45" Margin="952,652,0,0" VerticalAlignment="Top" Width="58"/>
            <Button Command="{Binding GetGenParametersCommand}" Background="Transparent" Cursor="Hand" CommandParameter="Gen_0006" HorizontalAlignment="Left" Height="45" Margin="437,295,0,0" VerticalAlignment="Top" Width="58"/>
            <Button Command="{Binding GetGenParametersCommand}" Background="Transparent" Cursor="Hand" CommandParameter="Gen_0008" HorizontalAlignment="Left" Height="45" Margin="1134,284,0,0" VerticalAlignment="Top" Width="58"/>
        </Grid>
    </materialDesign:Card>

</Window>
